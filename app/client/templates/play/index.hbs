<section class="page-title dark-bg page-bg">
  <div class="overlay" style="min-height:0%">
    <div class="container">
      <div class="section-title">
        <h2>Play</h2>
        <div class="line"></div>
        <p>play html5 games on your browser</p>
      </div>
    </div>
  </div>
</section>

<section class="content-block default-bg">
  <div class="container">
    <div class="row">
      <div class="col-sm-10 col-md-10">
        <div>
          <div id="game-area"></div>
          <div id="game-ui">
            <button type="button" class="btn btn-" data-toggle="tooltip" title="Fullscreen" {{action 'fullscreen'}}>
              <span class="fa fa-square-o fa-2x"></span>
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
